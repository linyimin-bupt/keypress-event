{
  "extends": ["tslint:recommended"],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "trailing-comma": true,
    "import-spacing": false,
    "no-multi-spaces": false,
    "member-ordering": false,
    "typedef-whitespace": false,
    "await-promise": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
  }
}
